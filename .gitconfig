[user]
	email = amartinez@zendesk.com
	name = Ana Martinez
[merge]
  tool = opendiff
[color]
    ui = auto
  [color "branch"]
    current = yellow reverse
    local = yellow
    remote = green
  [color "diff"]
    meta = yellow bold
    frag = magenta bold
    old = red bold
    new = green bold
  [color "status"]
    added = yellow
    changed = green
    untracked = cyan
[branch]
    autosetuprebase = always
[github]
	user = anamartinez
	token = 5745e735b80e3b5bc7013170f3a81fb4
[alias]
    st = status
    ci = commit
    br = branch
    co = checkout
    df = diff
    dc = diff --cached
    lg = log -p
    l = log --graph --decorate --pretty=oneline --abbrev-commit
    lola = log --graph --decorate --pretty=oneline --abbrev-commit --all
    ls = ls-files
[push]
	default = upstream
[core]
	excludesfile = /Users/acemacu/.gitignore_global
[git-pair "authors"]
